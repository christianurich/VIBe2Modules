from pyvibe import *
import pyvibe
import random

class RandomDouble(Module):
    def __init__(self):
        Module.__init__(self)
        self.DoubleData = 0
        self.addParameter(self, "DoubleData", VIBe2.DOUBLEDATA_OUT)
    def run(self):
        self.DoubleData = random.randint(0,100)
        
        